{
  "name": "authgear-portal",
  "version": "0.1.0",
  "scripts": {
    "test": "jest",
    "start": "parcel serve --port 1234 --no-cache ./src/index.html",
    "build": "PARCEL_BUNDLE_ANALYZER=1 parcel build --no-cache ./src/index.html",
    "clean": "rm -rf ./dist/ ./parcel-bundle-reports/ && mkdir ./dist/ && touch ./dist/.gitkeep",
    "typecheck": "tsc",
    "fmt": "prettier --list-different --write './src/**/*.{js,ts,jsx,tsx,css,scss,html}'",
    "eslint": "eslint './src/**/*.{js,ts,jsx,tsx}'",
    "stylelint": "stylelint './src/**/*.{css,scss}'",
    "gentype": "graphql-codegen --config ./graphql.portal.codegen.yaml && graphql-codegen --config ./graphql.adminapi.codegen.yaml"
  },
  "alias": {
    "process": false,
    "buffer": false
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@oursky/eslint-plugin": "6.0.0",
    "@oursky/stylelint-oursky": "2.0.0",
    "@parcel/css": "1.10.1",
    "@parcel/transformer-sass": "2.6.2",
    "@types/deep-equal": "1.0.1",
    "@types/jest": "28.1.3",
    "@types/js-yaml": "4.0.5",
    "@types/luxon": "2.0.9",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/uuid": "8.3.4",
    "@types/zxcvbn": "4.4.1",
    "eslint": "7.32.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "normalize.css": "8.0.1",
    "parcel": "2.6.2",
    "prettier": "2.7.1",
    "sass": "1.53.0",
    "stylelint": "13.13.1",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@authgear/web": "0.17.1",
    "@fluentui/react": "8.77.1",
    "@graphql-codegen/near-operation-file-preset": "2.2.14",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-operations": "2.4.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.16",
    "@monaco-editor/react": "3.7.2",
    "@oursky/react-messageformat": "2.0.2",
    "axios": "0.27.2",
    "base64-js": "1.5.1",
    "chart.js": "3.8.0",
    "chartjs-plugin-datalabels": "2.0.0",
    "classnames": "2.3.1",
    "cropperjs": "1.5.12",
    "deep-equal": "2.0.5",
    "google-libphonenumber": "3.2.28",
    "graphql": "15.6.0",
    "history": "5.3.0",
    "i18n-iso-countries": "7.3.0",
    "immer": "9.0.12",
    "intl-tel-input": "17.0.15",
    "js-yaml": "4.1.0",
    "luxon": "2.3.0",
    "monaco-editor": "0.21.3",
    "postcss": "8.4.6",
    "react": "17.0.2",
    "react-chartjs-2": "4.0.1",
    "react-code-blocks": "0.0.9-0",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.2.2",
    "react-router-dom": "6.3.0",
    "tzdata": "1.0.29",
    "uuid": "8.3.2",
    "zxcvbn": "4.4.2"
  }
}
